{
  "non-interactive": false,
  "dry-run": false,
  "verbose": false,
  "force": false,
  "preReleaseId": null,
  "buildCommand": false,
  "safeBump": false,
  "src": {
    "beforeStartCommand": false,
    "afterReleaseCommand": false
  },
  "npm": {
    "publish": true
  },
  "git": {
    "requireUpstream": true,
    "requireCleanWorkingDir": false,
    "commit": true,
    "commitArgs": "",
    "tag": true,
    "push": true,
    "pushArgs": ""
  },
  "github": {
    "release": true
  },
  "prompt": {
    "src": {
      "status": false,
      "commit": true,
      "tag": true,
      "push": false,
      "release": false
    }
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "conventionalcommits",
      "infile": "CHANGELOG.md"
    }
  }
}
